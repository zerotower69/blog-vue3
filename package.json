{
  "name": "blog-vue3",
  "version": "1.0.0",
  "description": "using vue@3 to create my blog by multiply pages!",
  "main": "index.js",
  "scripts": {
    "prepare": "npx husky-init && husky install",
    "dev": "npx webpack serve --config ./build/webpack.dev.js --json > static.json",
    "analyzer": "npx webpack --config ./build/webpack.analyzer.js",
    "build": "npx webpack --config ./build/webpack.prod.js",
    "lint": "lint-staged",
    "test": "jest  --passWithNoTests"
  },
  "lint-staged": {
    "*.{ts,js,vue,jsx,tsx}": [
      "prettier --write",
      "tslint --cache --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint  --passWithNoTests",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "vue3",
    "blog",
    "zerotower"
  ],
  "repository": "https://www.github.com/zerotower69/blog-vue3",
  "author": "zerotower<zerotower@163.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/vue": "^2.0.0",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/test-utils": "^2.0.0-rc.18",
    "babel-jest": "^27.5.1",
    "commitizen": "^4.2.4",
    "css-loader": "^5.2.7",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.12.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^1.6.2",
    "naive-ui": "^2.26.4",
    "postcss": "^8.4.12",
    "postcss-cssnext": "^3.1.1",
    "postcss-loader": "^4.2.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.3.0",
    "typescript": "^4.6.3",
    "vfonts": "^0.0.3",
    "vue-jest": "^3.0.7",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "4",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "vue": "3",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  }
}
